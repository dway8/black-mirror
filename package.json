  "private": true,
  "scripts": {
    "lint": "cd server && yarn lint && cd .. && yarn lint",
    "dev": "webpack-dev-server --mode=development --hot --inline --config webpack.config.js --port 42424 --host 0.0.0.0 --public 0.0.0.0:42424",
