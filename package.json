    "eslint_d": "^5.0.0",
    "glob": "^5.0.14",
    "prettier": "^1.16.4",
    "prettier_d": "^5.7.4",
    "webpack-dev-server": "^3.1.4",
    "yadda": "^1.5.0"
  },
  "dependencies": {
    "copy-webpack-plugin": "^5.0.3",
